#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, temp;
    cout<<"enter a number" <<endl;
    cin>>n;
    temp = n;
    int count = 0;
    while(n!=0)
    {
        n = n/10;
        count++;
    }
    cout<<"number of digits in an integer is : " <<" " << count <<endl;
    return 0;
}

enter a number
12345
number of digits in an integer is :  5

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int year;
    cout<<"enter a year" <<endl;
    cin>>year;
    if(year%4==0)
    {
        if(year%100==0)
        {
            if(year%400==0)
            cout<<"leap year" <<endl;
            else
            cout<<"not a leap year" <<endl;
        }
        else
        cout<<"Leap year" <<endl;
        }
        else
        cout<<"Not a leap year" <<endl;

    return 0;
}

enter a year
2024
Leap year

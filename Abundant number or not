#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"enter a number"<<endl;
    cin>>n;
    int i = 1, sum = 0;
    for(i=1; i<=n; i++)
    {
        if(n%i==0)
        {
            sum = sum + i;
        }
    }
    if(n<sum)
    cout<<"Abundant number" <<endl;
    else
    cout<<"not an abundant number" <<endl;
    return 0;
}

enter a number
12
Abundant number

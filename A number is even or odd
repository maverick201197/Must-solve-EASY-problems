#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"enter a number" <<endl;
    cin>>n;
    if(n%2==0)
    {
        cout<<"Even Number" <<endl;
    }
    else
    {
        cout<<"Odd Number" <<endl;
    }
    return 0;
}

enter a number
12
Even Number

enter a number
13
Odd Number

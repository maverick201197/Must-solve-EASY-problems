#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,gcd;
    cout<<"enter two numbers" <<endl;
    cin>>a>>b;
    int i;
    for(i=1; i<=a && i<=b; i++)
    {
     if((a%i==0) && (b%i==0))
     {
         gcd = i;
     }
    }
     cout<<gcd<<endl;

    return 0;
}

enter two numbers
20 28
4

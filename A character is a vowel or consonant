#include<bits/stdc++.h>
using namespace std;
int main()
{
    char ch;
    cout<<"enter a character" <<endl;
    cin>>ch;
    if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' || ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U')
    {
        cout<<"Vowels" <<endl;
    }
    else
    {
        cout<<"Consonants" <<endl;
    }
    return 0;

}

enter a character
j
Consonants

enter a character
i
Vowels

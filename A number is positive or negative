#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"enter a number" <<endl;
    cin>>n;
    if(n>=0)
    {
        cout<<"Positive Number" <<endl;
    }
    else
    {
        cout<<"Negative Number" <<endl;
    }
    return 0;
}

enter a number
20
Positive Number

enter a number
-10
Negative Number

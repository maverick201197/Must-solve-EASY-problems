#include<bits/stdc++.h>
using namespace std;
int main()
{
    int num1,num2,sum1=0,sum2=0,i;
    cout<<"enter two numbers" <<endl;
    cin>>num1>>num2;
    for(int i=1;i<num1;i++)
    {
        if(num1%i==0)
        {
            sum1 = sum1 + i;
        }
    }
    
    for(int i=1; i<num2; i++)
    {
        if(num2%i==0)
        {
            sum2 = sum2 + i;
        }
    }
    
    if((sum1==num1) && (sum2==num2))
    cout<<"Friendly Number" <<endl;
    else
    cout<<"Not a Friendly Number" <<endl;
    return 0;
}

enter two numbers
6 28
Friendly Number

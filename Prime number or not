#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    cout<<"enter a number" <<endl;
    cin>>n;
    for(int i=2; i<=n/2; i++)
    {
        if(n%i==0)
        {
        break;
        }
    }
    if(i>n/2)
    cout<<"prime number"<<endl;
    else
    cout<<"not a prime number" <<endl;
    return 0;
}

enter a number
13
prime number
